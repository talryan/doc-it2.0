<%=form_tag('/doctors', method: 'get') do %>
<%=label_tag(:q, "Search By State:") %>
<%=text_field_tag(:q)%>
<%= submit_tag("Search")%>
<% end %>



<% @doctors.each do |doctor| %>
<ul>
<li> <h3><%= doctor.state %></h3>
<%= link_to "#{doctor.name}", doctor_path(doctor.id)%>
<li><label> (<%= doctor.specialty%>)</label>
</ul>
<br> 
<%end%>

