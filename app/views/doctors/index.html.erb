<%=form_tag('/doctors', method: 'get') do %>
<%=label_tag(:q, "Search By Specialty:") %>
<%=text_field_tag(:q)%>
<%= submit_tag("Search", class: 'btn btn-dark')%>
<% end %>






<% @doctors.each do |doctor| %>
<h4> <%= doctor.state%> </h4>
<%= link_to "#{doctor.name}", doctor_path(doctor)%>
<li><label> (<%= doctor.specialty%>)</label>
</ul>
<br> 
<%end%>


