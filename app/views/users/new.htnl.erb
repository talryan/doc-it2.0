<h1> Sign Up </h1>

<%= form_for :user do |f|%>
    <%= f.label "First Name"%> <br>
    <%= f.text_field :first_name %> <
    <%= f.label "Last Name" %>
    <%= f.text_field :last_name %>
    <%= f.label :Email %>
    <%= f.email_field :email %>
    <%= f.label :Password %>
    <%= f.password_field :password %>
    <%= f.submit "Sign Up" %>
    <% end %>



