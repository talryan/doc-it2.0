
<h1> Sign Up </h1>

<%= form_for :user do |f|%>
    <%= f.label "First Name:"%> <br>
    <%= f.text_field :first_name %> <br>
    <%= f.label "Last Name:" %><br>
    <%= f.text_field :last_name %><br>
    <%= f.label "Email:" %><br>
    <%= f.email_field :email %><br>
    <%= f.label "Password:" %><br>
    <%= f.password_field :password %><br>
    <%= f.submit "Sign Up" %>
    <% end %>



